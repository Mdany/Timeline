<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.monster.timeline"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/timeline"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="org.monsterlab.timeline.HeadPage" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.monsterlab.timeline.StatusActivity" >
        </activity>
        <activity android:name="org.monsterlab.timeline.PhotoActivity" >
        </activity>
        <activity android:name="org.monsterlab.timeline.WTextActivity" >
        </activity>
        <activity android:name="org.monsterlab.photo_text.ImageEnlargedActivity" >
        </activity>
        <activity android:name="org.monsterlab.install.TabDemo">
        </activity><activity android:name="org.monsterlab.install.Alert"></activity>
        
        <activity android:name="org.monsterlab.install.InstallActivity"></activity>
        <receiver android:name="org.monsterlab.install.Call"></receiver>
        <activity android:name="org.monsterlab.timeline.MiddleActivity"></activity>
    </application>

</manifest>